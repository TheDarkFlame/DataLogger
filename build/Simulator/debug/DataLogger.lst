                      00047 delay_10_ms macro
                      00048     ;goto=2 cyles, decfsz=1 cycle, if zero=2 cycles
                      00049     MOVLW .40
                      00050     MOVWF D2
                      00051 
                      00052 ;if innerloopnumber=82, total cycles=10082, which is close enough to 10ms
                      00053     MOVLW .83   ;approx 1/4 ms
                      00054     MOVWF D1    
                      00055     
                      00056     DECFSZ D1,F ;count until D1=0
                      00057     GOTO $-1    ;keep counting down until
                      00058     DECFSZ D2,F ;D1=0, so count down D2, and reset D1 until D2=0
                      00059     GOTO $-4    ;reset D1
                      00060     ENDM
                      00061 ;-------------------------------------------------------------------------------
                      00062 
                      00063     
                      00064 ;-------------------------------------------------------------------------------
                      00065 eeprom.store macro edata, eadr
                      00066  ;does the eeprom store command, simple saves "edata" into "eadr" in eeprom
                      00067     BANKSEL eadr
                      00068     MOVFW eadr ;move CUR_EEADR into W
                      00069     BANKSEL EEADR
                      00070     MOVWF EEADR     ;move W into EEADR
                      00071     BANKSEL PORTC
                      00072     MOVFW edata     ;move our data into W
                      00073     BANKSEL EEDAT
                      00074     MOVWF EEDAT     ;move W into EEDAT
                      00075     BANKSEL EECON1
                      00076     BCF EECON1, EEPGD;data memory
                      00077     BSF EECON1, WREN;enable writes    
                      00078     
                      00079     MOVLW 0x55
                      00080     MOVWF EECON2    ;write 0x55 to EECON2
                      00081     MOVLW 0xAA
