                      00083     BSF EECON1,WR   ;begin write
                      00084     
                      00085     BTFSC EECON1,WR ;check for end of write
                      00086     GOTO $-1        ;loop until clear(Write complete)
                      00087     BCF EECON1, WREN;disable writes
                      00088     BANKSEL 0x00    ;move back to bank0
                      00089     
                      00090     ENDM
